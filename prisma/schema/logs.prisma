model ReelsLog {
  id        String   @id @default(uuid())
  reelName  String 
  userId    String
  status    String
  mensaje   String? 
  insertedAt DateTime @default(now())
  
  user       User     @relation(fields: [userId], references: [id])
}

model Logs {
  id        String   @id @default(uuid())
  tipoLog   String
  statusCode    Int
  mensaje   String? 
  insertedAt DateTime @default(now())
}